    "@types/styled-components": "^4.1.18",
    "eslint-config-react-app": "^4.0.1",
    "prettier": "^1.18.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CarlBrownDatacom/gatsby-kick-starter"
  },
